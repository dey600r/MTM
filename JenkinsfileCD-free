node {
  stage('SCM') {
    checkout scm
  }
   stage('Android - Build') {
     sh 'docker run --rm -v docker_jenkins_data:/home/workspace node-android bash -c "cd ./workspace/deyapps-mtm-build/app && npm run build:release-free-jenkins"'
  }
  stage('Save Artifacts') {
    archiveArtifacts artifacts: 'Utils/Versions/Android/**/*.apk, Utils/Versions/Android/**/*.aab'
  }
}
