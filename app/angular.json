{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "app",
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "www",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              { "glob": "**/*bar-chart.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*caret-down-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*caret-up-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*bicycle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*navigate-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*notifications-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*checkmark-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*checkmark-done-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*warning.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*nuclear.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*skull.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*success.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*cog.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*add-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*lock-closed.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*trash.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*disc.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*basket.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*flash.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*thermometer.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*color-fill.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*layers.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*eyedrop.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*settings.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*battery-charging.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*barcode.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*cube.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*bulb.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*bandage.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*briefcase.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*information-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*eye.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*filter.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*construct.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*build.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*hammer.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*shirt.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*gift.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*body.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*repeat.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*close-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*alert.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*save.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*alarm.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*calendar.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*stats-chart.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*navigate.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*analytics.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*arrow-down-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*arrow-back-sharp.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*search-sharp.svg ", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*close-sharp.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*walk.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*trail-sign.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*home.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*car-sport.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*car.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*reload-circle.svg", "input": "node_modules/ionicons/dist/ionicons/svg", "output": "./svg" },
              { "glob": "**/*.svg", "input": "src/assets/icon", "output": "./svg" }
            ],
            "styles": [
              {
                "input": "src/theme/variables.scss"
              },
              {
                "input": "src/global.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "production-free": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment-free.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "production-free": {
              "browserTarget": "app:build:production-free"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app:serve:production"
            },
            "production-free": {
              "devServerTarget": "app:serve:production-free"
            },
            "ci": {
              "devServerTarget": "app:serve:ci"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "production-free": {
              "browserTarget": "app:build:production-free"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "app:ionic-cordova-build",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "app:ionic-cordova-build:production",
              "devServerTarget": "app:serve:production"
            },
            "production-free": {
              "cordovaBuildTarget": "app:ionic-cordova-build:production-free",
              "devServerTarget": "app:serve:production-free"
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@ionic/angular-toolkit"
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}
